</div>
<div class="container" id="videos">
  <div class="row">
      <div class="col-md-4">
        <h3 class="text-muted">Free Videos</h3>
        <% @freevideos.each do |video| %>
          <%= render partial: 'video', locals: {video: video} %>
        <% end %>
      </div>
      <div class="col-md-4">
        <h3 class="text-muted">For Beginners</h3>
        <% @beginnervideos.each do |video| %>
          <%= render partial: 'video', locals: {video: video} %>
         <% end %>
      </div>
      <div class="col-md-4">
        <h3 class="text-muted">Pro Videos</h3>
        <% @advancedvideos.each do |video| %>
          <%= render partial: 'video', locals: {video: video} %>
        <% end %>
      </div>
  </div>
