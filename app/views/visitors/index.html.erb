<!-- Page Content -->
<%= render :partial => 'form' %>
<div class="container">
  <div class="container text-center">
    <div class="row">
      <div class="col-md-2">
      </div>
      <div class="col-md-8">
        <br />
        <script charset="ISO-8859-1" src="//fast.wistia.com/assets/external/E-v1.js" async></script><div class="wistia_responsive_padding" style="padding:56.25% 0 0 0;position:relative;"><div class="wistia_responsive_wrapper" style="height:100%;left:0;position:absolute;top:0;width:100%;"><span class="wistia_embed wistia_async_<%= @thisweekvideo.wistia %> popover=true popoverAnimateThumbnail=true videoFoam=true" style="display:inline-block;height:100%;width:100%">&nbsp;</span></div></div>
      </div>
    </div>
    <div class="col-md-2">
    </div>
  </div>
  <h3 class="text-center">A new FREE video every week.</h4>
